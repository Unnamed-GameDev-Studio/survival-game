{
  "name": "postapoc",
  "version": "0.0.22",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "jest",
    "test:e2e": "jest --config=jest.e2e.config.js",
    "test:e2e:serve": "concurrently \"yarn serve\" \"yarn test:e2e\"",
    "coverage": "jest --coverage --collectCoverageFrom=\"./src/**\"",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write .",
    "postinstall": "husky install",
    "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
    "commitlog": "conventional-changelog -p angular -i COMMITLOG.md -s -r 0\n",
    "index": "ctix create -p ./tsconfig.json",
    "update_comments": "node .husky/hooks/add_comments.js"
  },
  "dependencies": {
    "bitecs": "^0.3.38",
    "inversify": "^6.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "mitt": "^3.0.0",
    "phaser": "3",
    "reflect-metadata": "^0.1.13",
    "tiled": "^0.3.0",
    "tweakpane": "^3.1.7",
    "winston": "^3.8.2"
  },
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/cz-commitlint": "^17.4.4",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "commitizen": "^4.3.0",
    "concurrently": "^7.6.0",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "^9.3.2",
    "husky": "^8.0.3",
    "inquirer": "^9.1.4",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-playwright-preset": "^3.0.1",
    "lint-staged": "^13.2.0",
    "playwright": "^1.32.0",
    "prettier": "^2.8.6",
    "prettier-eslint": "^15.0.1",
    "ts-jest": "^29.0.5",
    "typescript-eslint-parser": "^22.0.0",
    "vite": "^4.2.1"
  }
}
